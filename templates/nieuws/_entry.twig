{% set entrytype = entry.type.handle %}
{% if entrytype == 'acties' %}
    {% include '/nieuws/_entryActies' %}
{% elseif entrytype == 'nieuws' %}
    {% include '/nieuws/_entryNieuws' %}
{% elseif entrytype == 'indexActies' %}
    {% include '/nieuws/_indexActies' %}
{% elseif entrytype == 'indexNieuws' %}
    {% include '/nieuws/_indexNieuws' %}
{% else %}
    {% include '/nieuws/index' %}
{% endif %}